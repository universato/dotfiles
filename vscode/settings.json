{
    "autocompleteSymbols.enabledLanguages": ["ruby"],
    "editor.renderControlCharacters": true,
    "editor.snippetSuggestions": "top",
    // "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "files.associations":
    {
        // "*.gemspec": "gemspec",
        // "Gemfile": "Gemfile",
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "gitlens.defaultDateFormat": "YYYY/MM/DD HH:mm",
    "gitlens.defaultDateShortFormat": "YYYY/MM/DD",
    "hamlLint.useBundler": true,
    "indentRainbow.errorColor": "rgba(128, 32, 32, 0.1)",
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[markdown]": {
        "files.trimTrailingWhitespace": false,
        "editor.renderWhitespace": "boundary"
    },
    "ruby.intellisense": "rubyLocate",
    "ruby.lint": {
        "rubocop": false
    },
    "ruby.useLanguageServer": true,
    "solargraph.definitions": true,
    "solargraph.diagnostics": true,
    "terminal.integrated.shell.osx": "/bin/zsh",
    "yaml.schemas": {
        "http://json.schemastore.org/circleciconfig": "/.circleci/config.yml"
    },
    "workbench.iconTheme": "vscode-icons",
    "snippetsByPattern.snippets": {
        "**/settings.json": {
            "short snippet": {
                "prefix": "ss",
                "body": ["\"$1\": {", "\t\"prefix\": \"$2\",", "\t\"body\": \"$3\",", "},"]
            }
        },
        "**/*_test.rb": {
            "assert_equal": {
                "prefix": "assert_equal",
                "body": "assert_equal"
            }
        },
        "**/app/controllers/*.rb": {
            "ApplicationController": {
                "prefix": "ApplicationController",
                "body": "ApplicationController"
            },
            "def index": {
                "prefix": "def index",
                "body": [
                    "def index",
                    "  $1",
                    "end"
                ]
            },
            "def update": {
                "prefix": "def update",
                "body": [
                    "def update",
                    "  $1",
                    "end"
                ]
            }
        },
        "**/app/views/*.erb": {
            "link_to": {
                "prefix": "link_to",
                "body": [
                    "link_to"
                ]
            }
        },
        "**/db/migrate/*.rb": {
            "add_column": {
                "prefix": "add_column",
                "body": ["add_column :${1:table_name}, :${2:column_name}, :${3:type}${4, :null: false}"]
            },
            "add_index": {
                "prefix": "add_index",
                "body": ["add_index :${1:table_name}, :${2:column_name}"]
            },
            "change_method": {
                "prefix": "change",
                "body": [
                    "def chnge",
                    "  add_column :${1:table_name}, :${2:column_name}, :${3:type}${4, :null: false}",
                    "end"
                ]
            },
            "remove_index": {
                "prefix": "remove_index",
                "body": ["remove_index :${1:table_name}, :${2:column_name}"]
            },
            "not null": {
                "prefix": "null",
                "body": "null: false"
            },
            "unique true": {
                "prefix": "unique",
                "body": "unique: true"
            }
        },
        "**/Gemfile": {
            "gems": {
                "prefix": "gems",
                "body": "gem '$1'"
            },
            "gemd": {
                "prefix": "gemd",
                "body": "gem \"$1\""
            },
            "image_processing": {
                "prefix": "image_processing",
                "body": "image_processing"
            },
            "kaminari": {
                "prefix": "kaminari",
                "body": "kaminari"
            },
            "sass-rails": {
                "prefix": "sass-rails",
                "body": "sass-rails"
            },
            "slack-notifier": {
                "prefix": "slack-notifier",
                "body": "slack-notifier"
            }
        },
        "**/*.gemspec": {
            "add_dependency": {
                "prefix": "add_dependency",
            },
            "add_development_dependency": {
                  "prefix": "add_development_dependency",
                  "body": "add_development_dependency $1"
            }
        },
        "**/.rubocop.yml": {
            "Enabed: false": {
                "prefix": "Enabed: false",
                "body": "Enabed: false"
            },
            "Exclude:": {
                "prefix": "Exclude",
                "body": [
                    "Exclude:",
                    "  - '$1'"
                ]
            }
        }
    },
    "window.zoomLevel": -1
}
